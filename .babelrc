{
    "presets": [
        "stage-1"
    ],
    "plugins": [
        "transform-vue-jsx",
        "syntax-dynamic-import"
    ],
    "env": {
        "browser": {
            "preset": [
                "env",
                {
                    "targets": {
                        "browsers": [
                            "last 2 versions",
                            "safari >= 7"
                        ]
                    }
                }
            ]
        },
        "node": {
            "preset": [
                "env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        }
    }
}